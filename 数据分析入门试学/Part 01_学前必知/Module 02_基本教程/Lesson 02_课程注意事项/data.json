          "id": 787733,
          "key": "ec486d59-ee5f-4435-8485-285abf43fdf7",
          "title": "课程保留与维护规则",
          "semantic_type": "Concept",
          "is_public": true,
          "user_state": {
            "node_key": "ec486d59-ee5f-4435-8485-285abf43fdf7",
            "completed_at": "2019-01-22T05:26:41.297Z",
            "last_viewed_at": "2019-07-22T03:14:38.554Z",
            "unstructured": null
